{
    "name": "kaue/calculo-api-php",
    "description": "PHP version of calculo-api",
    "type": "project",
    "require": {
        "php": "^8.2",
        "workerman/workerman": "^4.0",
        "doctrine/dbal": "^3.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "vlucas/phpdotenv": "^5.5",
        "symfony/http-foundation": "^6.0",
        "react/event-loop": "^1.3",
        "react/http": "^1.9",
        "monolog/monolog": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "start": "php src/index.php",
        "dev": "php -S localhost:145 src/index.php"
    }
}